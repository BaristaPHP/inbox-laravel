{
    "name": "barista/laravel-inbox",
    "description": "Dummy Description",
    "autoload": {
        "psr-4": {
            "Barista\\LaravelInbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Barista\\LaravelInbox\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Barista",
            "email": "contact@barista-php.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "pestphp/pest": "^2.34"
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                ":Vendor\\:Package\\:PackageServiceProvider"
            ],
            "aliases": {
                ":Package": ":Vendor\\:Package\\Facades\\:Package"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}